}

.copyright2 {
	FONT-FAMILY: Verdana;
}


#product {
	MARGIN: 0px
}

#product SPAN {
	VISIBILITY: hidden
}

#container1 A {
	COLOR: #0685ff;
	TEXT-DECORATION: none
}

A:visited {
	COLOR: #0685ff;
	TEXT-DECORATION: underline
}

#container1 A:hover {
	COLOR: #ff8c00;
	TEXT-DECORATION: underline
}

#language A {
	COLOR: #e3effb;
	TEXT-DECORATION: none
}

#language A:visited {
	COLOR: #e3effb;
	TEXT-DECORATION: none
}

#language A:hover {
	COLOR: #ff8c00;
	TEXT-DECORATION: underline
}

#version {
