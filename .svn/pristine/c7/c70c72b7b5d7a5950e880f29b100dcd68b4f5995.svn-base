}

#container1 {
	BACKGROUND: url(../images/logo/logo_login.gif) #013d83 no-repeat;	
	MARGIN-TOP: 120px;	
	PADDING-RIGHT: 1em;
	PADDING-LEFT: 1em;
	PADDING-BOTTOM: 0px;
	PADDING-TOP: 0px;
	WIDTH: 511px;
	POSITION: relative;
	HEIGHT: 316px;
	TEXT-ALIGN: left
}

#loginform {
	PADDING-RIGHT: 16px;
	PADDING-TOP: 60px;
	HEIGHT: 100px;
