	POSITION: absolute;
	TEXT-ALIGN: right
}

HR {
	BORDER-TOP: #bbdeff 1px dotted;
	WIDTH: 220px;
	HEIGHT: 1px
}

#errorMsg {
	color: red;
	position: absolute;
	top: 180px;
	right: 30px;
	font-size:11px;
}

#tip{	
	COLOR: #d8e6f7;	
