/*****************************************************
 *  DISPLAY MESSAGES	                             *
 *****************************************************/
	/* normal and erro message-box */
.normal-message {
	position: relative;
	margin: 10px auto;
	margin-left: -250px;
	width: 500px;
	left: 50%;
	right: 50%;
	border-width: 1px;
	border-style: solid;
	padding: 5px;
	border: 1px solid #4171B5;
	color: #000;
	background-color: #E5EDF9;
	min-height: 30px;
}

.warning-message {
	position: relative;
	margin: 10px auto;
	margin-left: -250px;
	width: 500px;
	left: 50%;
	right: 50%;
	border-width: 1px;
	border-style: solid;
	padding: 5px;
	border: 1px solid #FFB30F;
	color: #000;
	background-color: #FFEFA7;
	min-height: 30px;
}

.confirmation-message {
	position: relative;
	margin: 10px auto;
	margin-left: -250px;
	width: 500px;
	left: 50%;
	right: 50%;
	border-width: 1px;
	border-style: solid;
	padding: 5px;
	border: 1px solid #1F8323;
	color: #000;
	background-color: #CAF0C7;
	min-height: 30px;
}

.error-message {
	position: relative;
	margin: 10px auto;
	margin-left: -250px;
	width: 500px;
	left: 50%;
	right: 50%;
	border-width: 1;
	border-style: solid;
	padding: 5px;
	border: 1px solid #FF0000;
