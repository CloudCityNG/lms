.Pagination {
	FONT-SIZE: 14px;
	TEXT-ALIGN: right;
	PADDING-BOTTOM: 5px;
	MARGIN: 5px 0px 0px;
	PADDING-LEFT: 0px;
	PADDING-RIGHT: 0px;
	PADDING-TOP: 5px;
	background-color:#FFF
}

.Pagination A {
	PADDING-BOTTOM: 0px;
	PADDING-LEFT: 7px;
	PADDING-RIGHT: 7px;
	MARGIN-LEFT: 2px;
	PADDING-TOP: 3px
}

.Pagination A:link {
	BORDER-BOTTOM: #ccc 1px solid;
	BORDER-LEFT: #ccc 1px solid;
	COLOR: #666;
	BORDER-TOP: #ccc 1px solid;
	BORDER-RIGHT: #ccc 1px solid;
	TEXT-DECORATION: none
}

.Pagination A:visited {
	BORDER-BOTTOM: #ccc 1px solid;
	BORDER-LEFT: #ccc 1px solid;
	COLOR: #666;
	BORDER-TOP: #ccc 1px solid;
	BORDER-RIGHT: #ccc 1px solid;
	TEXT-DECORATION: none
}

.Pagination A:hover {
	BORDER-BOTTOM: #4398ce 1px solid;
	BORDER-LEFT: #4398ce 1px solid;
	BACKGROUND: #02377D;
	COLOR: #fff;
	BORDER-TOP: #4398ce 1px solid;
	BORDER-RIGHT: #4398ce 1px solid
}

.Pagination A:active {
	BORDER-BOTTOM: #4398ce 1px solid;
	BORDER-LEFT: #4398ce 1px solid;
	BACKGROUND: #02377D;
	COLOR: #fff;
	BORDER-TOP: #4398ce 1px solid;
	BORDER-RIGHT: #4398ce 1px solid
}

.Pagination .current,.currentBtn {
	display: inline;
	PADDING-BOTTOM: 0px;
	PADDING-LEFT: 7px;
	PADDING-RIGHT: 7px;
	MARGIN-LEFT: 2px;
	PADDING-TOP: 3px;
	BACKGROUND: #02377D;
	COLOR: #fff;
	font-weight: bold;
	BORDER: #4398ce 1px solid;
}

.Pagination .current A:link {
	BORDER-BOTTOM: #4398ce 1px solid;
	BORDER-LEFT: #4398ce 1px solid;
	BACKGROUND: #4398ce;
	COLOR: #fff;
	BORDER-TOP: #4398ce 1px solid;
	BORDER-RIGHT: #4398ce 1px solid
}

.Pagination .current A:visited {
	BORDER-BOTTOM: #4398ce 1px solid;
	BORDER-LEFT: #4398ce 1px solid;
	BACKGROUND: #4398ce;
	COLOR: #fff;
	BORDER-TOP: #4398ce 1px solid;
	BORDER-RIGHT: #4398ce 1px solid
}
