@charset "utf-8";
/* CSS Document */

/* Reset Css */
/***********************************************************************************************************************/
*
{ margin:0; padding:0; font-size: 100%; text-decoration:none; }
img
{ border:0;}

ol,ul
{ list-style:none;}
/* tables still need 'cellspacing="0"' in the markup */
table
{ border-collapse:collapse; border-spacing:0;}
a:hover
{ color:#f15d21; text-decoration:underline;}
a:link 
{}
a:visited
{}

body {
	background-color: #fff;
	color: #000;
	font-family: 微软雅黑, V Trebuchet MS, Verdana, Arial, Helvetica, Lucida,
		sans-serif;
	font-size: 13px;
	margin: 0;
	padding: 0;
}

