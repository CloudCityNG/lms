/* Button Css */
/***********************************************************************************************************************/
.btn_login
{ height:36px; width:95px; background:url(images/login_btn.gif); overflow:hidden;border:0 none; cursor:pointer;}
.btn_rig
{ height:36px; width:95px; background:url(images/rig_btn.gif); overflow:hidden;border:0 none; cursor:pointer;}
.btn_querenbaocun
{ height:35px; width:130px; background:url(images/save.gif); overflow:hidden;border:0 none; cursor:pointer;}
input
{ padding:3px; margin-top:2px;  border:0px solid #AAAAAA; vertical-align:middle}
input[type=text],input[type=password],textarea{
	background:url(images/inputBg.gif) repeat-x scroll left top #FFF;
	border:1px solid #AAAAAA;
}
.btn_register
{ height:33px; width:100px; background:url(images/register.jpg); overflow:hidden;border:0 none; cursor:pointer;}
input.text
{ width:235px; height:20px; padding:6px 3px 9px; border:1px solid #BBBBBB; line-height:16px; float:left;}
input.radio,input.checkbox
{ margin:6px;}
