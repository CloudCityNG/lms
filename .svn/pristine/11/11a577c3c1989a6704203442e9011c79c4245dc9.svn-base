  .flex-control-nav{
    width: 100%;
    position: absolute;
    bottom: 20px;
    text-align: center;
}
.flex-control-nav li {
margin: 0 6px;
display: inline-block;
zoom: 1;
}

.flex-control-nav li a {
width: 11px;
height: 11px;
display: block;
background:#666;
/*background: rgba(0,0,0,0.5);*/
cursor: pointer;
text-indent: -9999px;
-webkit-border-radius: 20px;
-moz-border-radius: 20px;
-o-border-radius: 20px;
border-radius: 20px;
box-shadow: inset 0 0 3px rgba(0,0,0,0.3);
}
.flex-control-nav li.on a {
background: #000;
background: rgba(0,0,0,0.9);
cursor: default;
}
