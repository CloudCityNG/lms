.rank-one-inner {
    padding:19px 0;
}
.ranking-one{
    background:#ea3232;
}
.ranking-one li,.second-rank .ranking-one{
    font-weight:bold;
    font-size:28px;
}
.ranking-two{
     background:#ed9e21;
}
.ranking-two li,.second-rank .ranking-two{
        font-size:26px;
}
.ranking-three {

    background:#00d9a3;
}
.ranking-three li,.second-rank .ranking-three{
        font-size:24px;
}
.ranking-four{

    background:#0f5c95;
}
.ranking-four li,.second-rank .ranking-four{
        font-size:20px;
}

.ranking-five{
    background:#CCC;
    opacity:0.8;
}