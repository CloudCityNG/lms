#grade_list{
    width:70%;
    background:#111d22;
    color:#00050C;
    border:1px solid #6B6C78;
     border-radius:6px;
    padding:20px auto;
     margin:0 auto;
}
.rank-list {
    width:90%;
    color:#fff;
    margin:10px auto 30px auto;
}

.rank-list ul {
    width:100%;    
    padding:0;
    margin:0;
    opacity:0.8;
   
}
.rank-list ul.ranking-four{
    border-bottom:1px solid #4B85B0;
}
.rank-list ul.ranking-five{
     border-bottom:1px solid #E4E4E4;
}
.rank-first{
       color:#fff;
       font-weight:bold;
}
.rank-width{
    width:100%;
     border-bottom:1px solid #48515A;
}
.rank-width ul{
    border:0;
}
 .rank-sec{
/*    background:#00B0F0;*/
}
.rank-sec:after{
    display:block;
    clear:both;
    content:"";
}

.rank-sec .rank-inner li{
    width:25%;
    padding:15px 0;
    font-size:26px;
}


.rank-list ul:after{
    clear:both;
    display:block;
    content:"";
}
.rank-list ul li{
    list-style:none;
    float:left;
   width:32.8%;
    padding:15px 0;
    text-align:center;
    position:relative;
}

.rank-list .rank-first li{
    font-size:24px;
}
.rank-sec li{
    font-size:22px;
    font-weight:bold;
}

.rank-sec .rank-pad,.rank-sec .rank-record-2,.rank-sec .rank-record-3{
   padding:10px 0 0 0;
}
