@charset "utf-8";


body{
   padding:0;
   margin:0;
   background:#fff;
   height:auto;
   font-size:14px;
    font-family: "Microsoft Yahei",Tahoma, Helvetica, Arial, "SimSun", sans-serif;
}

#login-head{
  height:70px;
  z-index:999;
  width:100%;
  background:url(../images/bg_nav_rep.png) repeat-x;
}
.g-flow{
    width:1045px;
    height:100%;
    margin:0 auto;
}
.g-flow:after{
    clear:both;
    display:block;
    content:'';
}
h1{
  display:block;
  font-size:2em;
  -webkit-margin-before: 0.67em;
  -webkit-margin-after: 0.67em;
  -webkit-margin-start: 0px;
  -webkit-margin-end: 0px;
  font-weight:bold;
  margin:0;
  padding:0;
}
#login-head .first-logo{
   height:60px;
   float:left;
}
#login-head  .learn-login{
    float:left;
    width:145px;
   height:70px;
    background:url("../images/home5.png") no-repeat 0 5px;
}
.b-20{
  width:100%;
  height:20px;
}
.b-15{
   width:100%;
   height:15px;
}
.b-10{
   width:100%;
   height:10px;
}

.g-doc{
  width:960px;
  margin:0 auto; 
  position:relative;
  margin-bottom:20px;
}
.g-doc:after{
    display:block;
    clear:both;
    content:"";
}
.m-logform{
    height:300px;
  padding:50px 0;
  background:url("../images/login-bg.gif") no-repeat 0 140px;
    height:369px;
}
.f-pr{
  position:relative;
}
.f-cb{
   zoom:1;
}
.right{
  float:right;
  margin-right:80px;
}
.m-loginbox{
  border: 1px solid #C5C5C5;
  box-shadow: 0 1px 6px #999;
  width:369px;
  text-align:left;
}
.m-loginbox .user-login{
   padding:15px 0 10px 20px; 
   border-bottom:1px solid #E8EAEF;
}
.login-content-input{
   padding:15px 20px;
}
form{
  display:block;
  margin-top:0em;
}
b{
   font-weight:bold;
   color:#444;
}
.buname,.bpasswd{
   display:block;
   width:100%;
   margin-top:5px;
}
input.l-input{
  height:40px;
  width:330px;
 border:1px solid #DEE3EA; 
  padding-left:30px;
  font-size:14px;
   padding:1px 0px;
    background: #f1f1f1;
   outline: 1px solid #dfdfdf; 
   text-indent:3em;
}
.uname{
  background:url("../images/loginBg.png") no-repeat 9px 11px;
}
.passwd{
  background:url("../images/loginBg.png") no-repeat 9px -26px;
}
input, input[type="password"]{
  -webkit-appearance: textfield;
  -webkit-rtl-ordering: logical;
   -webkit-user-select: text;
  cursor: auto;
}
input{
  color:#333;
  outline:0;
}
input{
  margin: 0em;
font: -webkit-small-control;
color: initial;
letter-spacing: normal;
word-spacing: normal;
text-transform: none;
text-indent: 0px;
text-shadow: none;
display: inline-block;
text-align: start;
 }
 input[type="checkbox"] {
-webkit-appearance: checkbox;
box-sizing: border-box;
margin:3px 3px 3px 4px;
}
 .itm-1{
    z-index:11;
	margin-top:10px;
	height:15px;
	line-height:15px;
	display:block;
 }
 .itm-1 .lb{
   float:left;
 }
 .itm-1 .lb .atlg{
   color:#999;
   font-size:12px;
 }
 .itm-1 a{
    font-size:12px;
	display:block;
	float:right;
	margin-right:10px;
	zoom:1;
	color:#61a500;
 }
 .m-loginbox .u-btn{
    height:44px;
	line-height:37px;
	width:100%;
	font-size:20px;
 }
 .u-btn-primary{
   font-family: "Microsoft Yahei",Tahoma, Helvetica, Arial, "SimSun", sans-serif;
   background:#00A750;
   background-image:linear-gradient(#4EA45B, #3C8440);
   color:#fff;
   text-align:center;
   border-radius:6px;
   border-color:#3C8440;
 }
 .u-btn-primary:hover{
    background:#4EA45B;
 }
 .reg a{
   color:#61a500;
   font-size:12px;
 }