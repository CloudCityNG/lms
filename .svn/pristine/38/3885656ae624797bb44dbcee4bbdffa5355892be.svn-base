body{
/*    min-width:1000px;*/
}
ul,li{
    padding:0;
    margin:0;
    list-style:none;
}
.container .container-safe{
     height:400px;
     position:relative;
     /*-width:expression(document.documentElement.clientWidth<960?960:document.documentElement.clientWidth);*/
     overflow:hidden;
}
.c-safe{
    height:400px;
}
.c-safe .c-img{
float: left;
width: 1680px;
height: 400px;
overflow: hidden;
}
.c-safe .c-img a{
   display: block;
   width: 1680px;
    height:400px;
}

.c-safe .c-img .box-img1{
    background:url("../../img/safety-1.png") no-repeat;
}
.c-safe .c-img .box-img2{
    background:url("../../img/safety-2.png") no-repeat;
}
.c-safe .c-img .box-img3{
    background:url("../../img/safety-3.png") no-repeat;
}
.flex-control-nav{
    width:400px;
    position: absolute;
    bottom: 70px;
    left:45%;
    text-align: center;
}
.flex-control-nav li {
margin: 0 6px;
display: inline-block;
float:left;
zoom: 1;
}

.flex-control-nav li a {
width: 11px;
height: 11px;
display: block;
background:#666;
cursor: pointer;
text-indent: -9999px;
-webkit-border-radius: 20px;
-moz-border-radius: 20px;
-o-border-radius: 20px;
border-radius: 20px;
box-shadow: inset 0 0 3px rgba(0,0,0,0.3);
}
.flex-control-nav li.on a {
background: #000;
background: rgba(0,0,0,0.9);
cursor: default;
}
.ourwork {
 width:100%;
 background: #f5f5f5;
}
.ourwork .our-in{
    width:970px;
    margin:0 auto;
}
.ourwork header{
    padding-bottom:30px;
}
.ourwork h1.our-title {
margin-top: 0;
color: rgb(39, 157, 217);
font-weight: 500;
padding-top: 20px;
