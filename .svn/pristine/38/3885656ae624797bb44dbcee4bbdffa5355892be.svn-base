/*视频列表*/

#c-container{
    width:100%;
   padding:10px 0;
}

#c-container:after{
    display:block;
    clear:both;
    content:"";
}
.container-video{
    margin:0 auto;
}
.video-list{
   width:100%;
}

.video-list:after{
    display:block;
    clear:both;
    content:"";
}
.v-outer{
    width:100%;
    height:20px;
}
.v-more{
    display:block;
    float:right;
    margin-right:23px;
    font-weight:bold;
    color:rgb(39,157,217);
}
.wrap-left {
margin-left: 0px;
}
.subject-wrap {
float: left;
border: 0px;
line-height: 10px;
margin-right:3%;
margin-top:5px;
}
.subject-wrap {
text-align: center;
padding-bottom: 10px;
margin-bottom: 20px;
}
.right-0{
    margin-right:0;
}
.subject-wrap a {
display: block;
cursor: pointer;
text-decoration: none;
outline: none;
}
a.subj-img-link {
height:185px;
display: block;
border:1px solid #ccc;
box-shadow:0 1px 4px #999;
}
.subject-wrap .newSubname{
    /*display:block;*/
    line-height:35px;
    font-size:14px;
}
.more-video{
    clear:both;
    display:block;
    float:right;
    margin-right:15px;
    font-size:16px;
    line-height:10px;
    
}

#xwdt {
position: relative;
padding-bottom: 60px;
padding-top:20px;
}
#xwdt {
width: auto;
height: auto;
background-color: #f5f5f5;
}
#xwdt:after{
    display:block;
    clear:both;
    content:"";
}
header.special {
text-align: center;
margin-bottom: 20px;
}
#xwdt h1 {
font-size: 22px;
font-weight: 600;
margin-bottom: 5px;
margin-top: 0;
color: rgb(39,157,217);
}
#xwdt .main-content-wrap {
    
margin-left: auto;
margin-right: auto;
margin:0 auto;
position: relative;
z-index: 11;
}

#xwdt h3 {
font-size:18px;
font-weight: 600;
margin-bottom: 5px;
margin-top: 0;
text-indent:1em;
color: rgb(39,157,217);
}
#xwdt .main-content {
float:left;
}
#xwdt ul {
list-style: none;
}
article.main-content ul {
padding-left: 35px;
}
#xwdt ul li {
list-style-image: url(../../img/sanjiao.png);
width: 100%;
height: 30px;
border-bottom: 1px dashed #000;
position: relative;
text-align:left;
}
article.main-content ul li {
font-size: 18px;
color: #454545;
line-height: 36px;
}
#xwdt a {
font-family: "黑体";
font-size: 15px;
color: rgb(92,93,93);
text-decoration: none;
}
.col-md-7 {
width: 600px;
float:left;
padding-left: 15px;
}
.col-md-7 p{
    font-size: 16px;
    color: #787878;
    line-height: 32px;
}
.hidden-xs{
    margin-bottom: 20px;
    margin-left: 70px;
    width: 33.33333333%;
    float: left;
}
.safe-test{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 1100;
    cursor: pointer; 
    height: 3124px; 
   display:none;
   background-color: rgba(119, 119, 119,0.7);
}
.safe-inner{
    position: absolute;
    /*top: 2195px;*/ 
    left: 400px; 
    padding:10px;
    z-index: 1101;
    outline: none;
    width: 758px;
    height:538px; 
    display:none;
    background: #fff;
}
.test-tip{
    border:1px solid #ccc;
    border-radius:5px;
}
.g-background{
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffffff), color-stop(100%,#EEEEEE));
    background: -webkit-linear-gradient(top, #FFFFFF 0%,#EEEEEE 100%);
    background: -o-linear-gradient(top, #FFFFFF 0%,#EEEEEE 100%);
    background: -ms-linear-gradient(top, #FFFFFF 0%,#EEEEEE 100%);
    background: linear-gradient(to bottom, #FFFFFF 0%,#EEEEEE 100%);
}
.test-tip .con-title{
      margin:5px 20px 10px 10px;
}
.test-tip .con-title:after{
    display:block;
    clear:both;
    content:"";
}

.con-title .safe-top{
    text-align:left;
    line-height:20px;  
}
.test-count{
    width:120px;
   line-height:20px;
   border-radius:10px;
   float:left;
   margin-top:6px;
   outline:0;
}
.point{
    float:right;
}
