/* Essentials */

html, div, map, dt, isindex, form, header, aside, section, section, article, footer {  
    display: block;  
} 

html, body {
    height: 100%;
    margin: 0;
    padding: 0;
    font-family: "Microsoft Yahei",Tahoma, Helvetica, Arial, "SimSun", sans-serif;
    background: #F8F8F8;
    font-size: 12px;
/*    min-width:1150px;*/
}
.clear {
    clear: both;
}
.spacer {
    height: 20px;
}

a:link, a:visited {
    color:#000;
    text-decoration: none;
}

a:hover {
    text-decoration: underline;
}
.hight{
    color:#FF0000;
    font-weight:bold;	
}
em{
    font-style:normal;    
}

