.vmip{
	margin:0 !important;
	display:block;
	width:100%;
	height:32px;
	background:url(../images/labtitibg.gif) repeat;
	line-height:32px;
	text-indent:2em;
} 

.vmContent{
	padding:15px;
	overflow:hidden;	
}
.vmContent dl{
	display:inline-block;
	float:left;	
	margin:20px;
}
.vmContent dl dt{
	width:37px;
	height:46px;
	background:url(../images/ipicon.gif) no-repeat;	
	margin:0 auto;
	line-height:39px;
	text-align:center;
	color:#fff;
	font-weight:bold;
}
.vmContent dl dd{
	margin:0;	
}
.vmContent dl dd a{
	font-weight:bold;
	color:#000000;
}
.vmContent dl dd a:hover{
	color:#F00;
}
.try-btn{
    padding:5px;
   float:right; 
}

.try-btn  a,.try-btn  a:hover{
    text-decoration:none;
}
.user-text{
    border-width: 1px;
-moz-border-radius-topright: 5px;
-moz-border-radius-topleft: 5px;
-moz-border-radius-bottomright: 5px;
-moz-border-radius-bottomleft: 5px;
font-size: 100%;
text-decoration: none;
color: black;
cursor: pointer;
padding: 2px 0px 2px 2px;
vertical-align: middle;
background-position: 10px;
background-repeat: no-repeat;
padding-left: 0px;
}
