/* Sidebar */

aside#sidebar {
width: 15%;
background: #E0E0E3 url(../images/sidebar.png) repeat;
float: left;
min-height: 500px;
position:relative;
}

#sidebar hr {
clear:both;
border: none;
outline: none;
background: url(../images/sidebar_divider.png) repeat-x;
display: block;
width: 100%;
height: 2px;
}
#flexButton{
position:absolute;
display:block;
width:12px;
height:26px;
right:-10px;
top:40%;
cursor:pointer;
}
.close{
	background:url(../images/CloseBroadside.gif) no-repeat;
}
.open{
	background:url(../images/OpenBroadside.gif) no-repeat;
}
/* Search */

.quick_search {
text-align: center;
padding: 14px 0 10px 0;
}
.quick_search input[type=text] {
-webkit-border-radius: 20px;
-moz-border-radius: 20px;
border-radius: 20px;
border: 1px solid #bbb;
height: 26px;
width: 90%;
color: #ccc;
-webkit-box-shadow: inset 0 2px 2px #ccc, 0 1px 0 #fff;
-moz-box-shadow: inset 0 2px 2px #ccc, 0 1px 0 #fff;
box-shadow: inset 0 2px 2px #ccc, 0 1px 0 #fff;
text-indent: 30px;
background: #fff url(../images/icn_search.png) no-repeat;
background-position: 10px 6px;
}

.quick_search input[type=text]:focus {
outline: none;
color: #666666;
border: 1px solid #77BACE;
-webkit-box-shadow: inset 0 2px 2px #ccc, 0 0 10px #ADDCE6;
-moz-box-shadow: inset 0 2px 2px #ccc, 0 0 10px #ADDCE6;
box-shadow: inset 0 2px 2px #ccc, 0 0 10px #ADDCE6;
}

